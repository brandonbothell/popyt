{
  "name": "popyt",
  "version": "5.0.0",
  "description": "A very easy to use promise-based Youtube Data v3 API.",
  "homepage": "https://bbothell.rocks/popyt",
  "main": "out/index.js",
  "types": "typings/index.d.ts",
  "scripts": {
    "test": "mocha --exit --require ts-node/register/transpile-only --require source-map-support/register --parallel --recursive \"./test/!(oauth).spec.ts\" --timeout 10000",
    "coverage": "tsc && cross-env NODE_ENV=test nyc yarn test && cross-env NODE_ENV=test nyc report --reporter=text-lcov | coveralls",
    "docs": "gulp typedoc",
    "prepublishOnly": "gulp",
    "lint": "eslint src/**/*.ts test/**/*.ts"
  },
  "author": "jasonhaxstuff",
  "license": "Unlicense",
  "keywords": [
    "youtube",
    "youtube search",
    "youtube api",
    "youtube videos",
    "youtube channels",
    "promise",
    "promise based",
    "no dependencies",
    "caching"
  ],
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "1.0.1",
    "@types/chai": "4.2.14",
    "@types/gulp": "4.0.7",
    "@types/gulp-sourcemaps": "0.0.34",
    "@types/merge2": "1.3.0",
    "@types/mocha": "8.2.1",
    "@types/node": "13.13.36",
    "@typescript-eslint/eslint-plugin": "4.9.1",
    "@typescript-eslint/parser": "4.9.1",
    "chai": "4.2.0",
    "coveralls": "3.1.0",
    "cross-env": "7.0.3",
    "dotenv": "8.2.0",
    "eslint": "7.14.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.8",
    "fs-nextra": "0.5.1",
    "gulp": "4.0.2",
    "gulp-sourcemaps": "3.0.0",
    "gulp-typedoc": "2.2.7",
    "gulp-typescript": "5.0.1",
    "merge2": "1.4.1",
    "mocha": "8.3.0",
    "nyc": "15.1.0",
    "source-map-support": "0.5.19",
    "ts-node": "9.1.0",
    "typedoc": "0.19.2",
    "typescript": "4.0.5"
  },
  "repository": {
    "url": "https://github.com/jasonhaxstuff/popyt.git"
  }
}
