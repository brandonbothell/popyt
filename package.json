{
  "name": "popyt",
  "version": "6.0.0-dev",
  "description": "A very easy to use promise-based Youtube Data v3 API.",
  "homepage": "https://bbothell.rocks/popyt/docs",
  "main": "out/index.js",
  "types": "typings/index.d.ts",
  "scripts": {
    "test": "mocha --exit --require ts-node/register/transpile-only --require source-map-support/register --parallel --recursive \"./test/!(oauth).spec.ts\" --timeout 10000",
    "coverage": "tsc && cross-env NODE_ENV=test nyc yarn test && cross-env NODE_ENV=test nyc report --reporter=text-lcov | coveralls",
    "docs": "gulp typedoc",
    "lint": "eslint src/**/*.ts test/**/*.ts"
  },
  "author": "jasonhaxstuff",
  "license": "Unlicense",
  "keywords": [
    "youtube",
    "youtube search",
    "youtube api",
    "youtube videos",
    "youtube channels",
    "promise",
    "promise based",
    "no dependencies",
    "caching"
  ],
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "1.0.2",
    "@types/chai": "4.3.5",
    "@types/gulp": "4.0.11",
    "@types/gulp-sourcemaps": "0.0.36",
    "@types/merge2": "1.4.1",
    "@types/mocha": "9.1.1",
    "@types/node": "20.3.1",
    "@typescript-eslint/eslint-plugin": "5.59.11",
    "@typescript-eslint/parser": "5.59.11",
    "chai": "4.3.7",
    "coveralls": "3.1.1",
    "cross-env": "7.0.3",
    "dotenv": "16.1.4",
    "eslint": "8.42.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsdoc": "46.2.6",
    "fs-nextra": "0.5.1",
    "gulp": "4.0.2",
    "gulp-sourcemaps": "3.0.0",
    "gulp-typescript": "6.0.0-alpha.1",
    "merge2": "1.4.1",
    "mocha": "9.2.2",
    "nyc": "15.1.0",
    "source-map-support": "0.5.21",
    "ts-node": "10.9.1",
    "typedoc": "0.24.8",
    "typedoc-neo-theme": "1.1.1",
    "typescript": "5.1.3"
  },
  "repository": {
    "url": "https://github.com/jasonhaxstuff/popyt.git"
  }
}
