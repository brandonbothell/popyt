{
  "extends": [
    "config:base"
  ],
  "packageRules": [
    {
      "updateTypes": [ "minor", "patch", "pin", "digest" ],
      "automerge": true
    }
  ],
  "encrypted": {
    "npmToken": ""
  },
  "hostRules": [
    {
      "matchHost": "https://registry.npmjs.org/",
      "hostType": "npm",
      "encrypted": {
        "token": ""
      }
    }
  ]
}
